{   
    "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "__dirname"
    },
    "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
    ],
    "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "indent": ["error", 2],
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
    }
}
